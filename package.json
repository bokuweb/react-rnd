{
  "name": "react-rnd",
  "version": "7.1.3",
  "description": "",
  "title": "react-rnd",
  "main": "./lib/index.es5.js",
  "module": "./lib/index.js",
  "jsnext:main": "./lib/index.js",
  "keywords": [
    "react",
    "resize",
    "resizable",
    "draggable",
    "component"
  ],
  "scripts": {
    "lint": "eslint src",
    "flow": "flow",
    "build:prod:main": "rollup -c scripts/prod.js",
